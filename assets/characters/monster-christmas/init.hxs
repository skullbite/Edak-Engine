function getIcon() return "monster";
function create() {
    char.barColor = 0xff78ff71;
    char.frames = Paths.getSparrowAtlas('monsterChristmas');
    addAnim('idle', 'monster idle');
    addAnim('singUP', 'monster up note', [-20, 50]);
    addAnim('singDOWN', 'monster down', [-40, -94]);
    // bruh
    addAnim('singRIGHT', 'Monster left note', [-30, 0]);
    addAnim('singLEFT', 'Monster Right note', [-51, 0]);
    
    playAnim('idle');
    
    char.displaceData.y = 130;
}