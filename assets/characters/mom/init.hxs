function create() {
    char.barColor = 0xfff191ba;
    char.frames = Paths.getSparrowAtlas("Mom_Assets");
    
    addAnim("idle", "Mom Idle");
    addAnim("singUP", "Mom Up Pose", [14, 71]);
    addAnim("singDOWN", "MOM DOWN POSE", [20, -160]);
    addAnim("singLEFT", "Mom Left Pose", [250, -23]);
    addAnim("singRIGHT", "Mom Pose Left", [10, -60]);
    
    playAnim("idle");
}