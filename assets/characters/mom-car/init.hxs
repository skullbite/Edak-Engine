package;

function getIcon() return "mom";
function create() {
    char.barColor = "0xf191ba";
    char.frames = Paths.getSparrowAtlas("momCar");
    addAnim("idle", "Mom Idle").addLoop([8, 9, 10, 11, 12, 13]);
    addAnim("singUP", "Mom Up Pose", [14, 71]);
    addAnim("singDOWN", "MOM DOWN POSE", [20, -160]);
    addAnim("singLEFT", "Mom Left Pose", [250, -23]);
    addAnim("singRIGHT", "Mom Pose Left", [10, -60]);

    playAnim("idle");
}